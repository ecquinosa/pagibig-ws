        Public Function SaveToDbase(ByVal DAL As DAL,
                                    ByRef myTrans As List(Of System.Data.SqlClient.SqlTransaction)) As Boolean
            If DAL Is Nothing Then
                pErrorMessage = "SaveMemberContactinfoRequest(): DAL is nothing"
                Return False
            End If

            If Not DAL.AddMemberContactinfo(pRefNum, pPagIBIGID, pPermanent_HBUR, pPermanent_Building, pPermanent_LotNo,
                             pPermanent_BlockNo, pPermanent_PhaseNo, pPermanent_HouseNo, pPermanent_StreetName,
                             pPermanent_Subdivision, pPermanent_Barangay, pPermanent_PSGC_Barangay_Code,
                             pPermanent_CityMunicipality, pPermanent_PSGC_City_Mun_Code, pPermanent_Province,
                             pPermanent_PSGC_Province_code, pPermanent_Region, pPermanent_PSGC_Region_Code,
                             pPermanent_ZipCode, pPresent_HBUR, pPresent_Building, pPresent_LotNo,
                             pPresent_BlockNo, pPresent_PhaseNo, pPresent_HouseNo, pPresent_StreetName,
                             pPresent_Subdivision, pPresent_Barangay, pPresent_PSGC_Barangay_Code, pPresent_CityMunicipality,
                             pPresent_PSGC_City_Mun_Code, pPresent_Province, pPresent_Province_code, pPresent_Region,
                             pPresent_PSGC_Region_Code, pPresent_ZipCode, pPreferredMailingAddress, pHome_CountryCode,
                             pHome_AreaCode, pHome_TelNo, pMobile_CountryCode, pMobile_AreaCode,
                             pMobile_CelNo, pBusiness_Direct_CountryCode, pBusiness_Direct_AreaCode, pBusiness_Direct_TelNo,
                             pBusiness_Trunk_CountryCode, pBusiness_Trunk_AreaCode, pBusiness_Trunk_TelNo, pEmailAddress, pBusiness_Trunk_Local, myTrans) Then

                pErrorMessage = "SaveMemberContactinfoRequest(): " & DAL.ErrorMessage
                Return False
            End If

            Return True
        End Function


        Public Function Load(ByVal DAL As DAL, ByVal refNumber As String) As Boolean
            Dim dt As DataTable = Nothing
            Try

                If DAL.SelectQuery(String.Format("SELECT * FROM tbl_MemberContactinfo WHERE RefNum = '{0}'", refNumber)) Then
                    dt = DAL.TableResult
                    If dt.Rows.Count > 0 Then
                        Dim value = dt.Rows(0)
                        pRefNum = refNumber
                        pPagIBIGID = value("PagIBIGID")
                        pPermanent_HBUR = IIf(value("Permanent_HBUR") Is DBNull.Value, String.Empty, value("Permanent_HBUR"))
                        pPermanent_Building = IIf(value("Permanent_Building") Is DBNull.Value, String.Empty, value("Permanent_Building"))
                        pPermanent_LotNo = IIf(value("Permanent_LotNo") Is DBNull.Value, String.Empty, value("Permanent_LotNo"))
                        pPermanent_BlockNo = IIf(value("Permanent_BlockNo") Is DBNull.Value, String.Empty, value("Permanent_BlockNo"))
                        pPermanent_PhaseNo = IIf(value("Permanent_PhaseNo") Is DBNull.Value, String.Empty, value("Permanent_PhaseNo"))
                        pPermanent_HouseNo = IIf(value("Permanent_HouseNo") Is DBNull.Value, String.Empty, value("Permanent_HouseNo"))
                        pPermanent_StreetName = IIf(value("Permanent_StreetName") Is DBNull.Value, String.Empty, value("Permanent_StreetName"))
                        pPermanent_Subdivision = IIf(value("Permanent_Subdivision") Is DBNull.Value, String.Empty, value("Permanent_Subdivision"))
                        pPermanent_Barangay = value("Permanent_Barangay")
                        pPermanent_PSGC_Barangay_Code = value("Permanent_PSGC_Barangay_Code")
                        pPermanent_CityMunicipality = value("Permanent_CityMunicipality")
                        pPermanent_PSGC_City_Mun_Code = value("Permanent_PSGC_City_Mun_Code")
                        pPermanent_Province = value("Permanent_Province")
                        pPermanent_PSGC_Province_code = value("Permanent_PSGC_Province_code")
                        pPermanent_Region = value("Permanent_Region")
                        pPermanent_PSGC_Region_Code = value("Permanent_PSGC_Region_Code")
                        pPermanent_ZipCode = value("Permanent_ZipCode")

                        pPresent_HBUR = IIf(value("Present_HBUR") Is DBNull.Value, String.Empty, value("Present_HBUR"))
                        pPresent_Building = IIf(value("Present_Building") Is DBNull.Value, String.Empty, value("Present_Building"))
                        pPresent_LotNo = IIf(value("Present_LotNo") Is DBNull.Value, String.Empty, value("Present_LotNo"))
                        pPresent_BlockNo = IIf(value("Present_BlockNo") Is DBNull.Value, String.Empty, value("Present_BlockNo"))
                        pPresent_PhaseNo = IIf(value("Present_PhaseNo") Is DBNull.Value, String.Empty, value("Present_PhaseNo"))
                        pPresent_HouseNo = IIf(value("Present_HouseNo") Is DBNull.Value, String.Empty, value("Present_HouseNo"))
                        pPresent_StreetName = IIf(value("Present_StreetName") Is DBNull.Value, String.Empty, value("Present_StreetName"))
                        pPresent_Subdivision = IIf(value("Present_Subdivision") Is DBNull.Value, String.Empty, value("Present_Subdivision"))

                        pPresent_Barangay = value("Present_Barangay")
                        pPresent_PSGC_Barangay_Code = value("Present_PSGC_Barangay_Code")
                        pPresent_CityMunicipality = value("Present_CityMunicipality")
                        pPresent_PSGC_City_Mun_Code = value("Present_PSGC_City_Mun_Code")
                        pPresent_Province = value("Present_Province")
                        pPresent_Province_code = value("Present_Province_code")
                        pPresent_Region = value("Present_Region")
                        pPresent_PSGC_Region_Code = value("Present_PSGC_Region_Code")
                        pPresent_ZipCode = value("Present_ZipCode")

                        pPreferredMailingAddress = IIf(value("PreferredMailingAddress") Is DBNull.Value, String.Empty, value("PreferredMailingAddress"))
                        pHome_CountryCode = IIf(value("Home_CountryCode") Is DBNull.Value, String.Empty, value("Home_CountryCode"))
                        pHome_AreaCode = IIf(value("Home_AreaCode") Is DBNull.Value, String.Empty, value("Home_AreaCode"))
                        pHome_TelNo = IIf(value("Home_TelNo") Is DBNull.Value, String.Empty, value("Home_TelNo"))

                        pMobile_CountryCode = IIf(value("Mobile_CountryCode") Is DBNull.Value, String.Empty, value("Mobile_CountryCode"))
                        pMobile_AreaCode = value("Mobile_AreaCode")
                        pMobile_CelNo = value("Mobile_CelNo")

                        pBusiness_Direct_CountryCode = IIf(value("Business_Direct_CountryCode") Is DBNull.Value, String.Empty, value("Business_Direct_CountryCode"))
                        pBusiness_Direct_AreaCode = IIf(value("Business_Direct_AreaCode") Is DBNull.Value, String.Empty, value("Business_Direct_AreaCode"))
                        pBusiness_Direct_TelNo = IIf(value("Business_Direct_TelNo") Is DBNull.Value, String.Empty, value("Business_Direct_TelNo"))
                        pBusiness_Trunk_CountryCode = IIf(value("Business_Trunk_CountryCode") Is DBNull.Value, String.Empty, value("Business_Trunk_CountryCode"))
                        pBusiness_Trunk_AreaCode = IIf(value("Business_Trunk_AreaCode") Is DBNull.Value, String.Empty, value("Business_Trunk_AreaCode"))
