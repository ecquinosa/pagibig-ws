        Public Function SaveToDbase(ByVal DAL As DAL,
                                    ByRef myTrans As List(Of System.Data.SqlClient.SqlTransaction)) As Boolean
            If DAL Is Nothing Then
                pErrorMessage = "SaveMembershipCategoryInfoRequest(): DAL is nothing"
                Return False
            End If

            If Not DAL.AddMembershipCategoryInfo(pRefNum, pPagIBIGID, pEmployeeID, pEmployerName, pEmployer_HBUR,
                             pEmployer_Building, pEmployer_LotNo, pEmployer_BlockNo, pEmployer_PhaseNo,
                             pEmployer_HouseNo, pEmployer_StreetName, pEmployer_Subdivision, pEmployer_Barangay,
                             pEmployer_psgc_Barangay_code, pEmployer_CityMunicipality, pEmployer_psgc_city_mun_code, pEmployer_Province,
                             pEmployer_psgc_Province_code, pEmployer_Region, pEmployer_psgc_region_code, pEmployer_ZipCode,
                             pDateEmployed, pEmployment_Status_code, pOccupation, pOccupationCode,
                             pAFPSerialBadgeNo, pDepEdDivCodeStnCode, pTypeOfWork, pIncome_Code,
                             pCountry_Assignment, pNatureOfWork, pOFWCountryCode, pEmpStatusCode, myTrans) Then
                pErrorMessage = "SaveMembershipCategoryInfoRequest(): " & DAL.ErrorMessage
                Return False
            End If

            Return True
        End Function

        Public Function Load(ByVal DAL As DAL, ByVal refNumber As String) As Boolean
            Dim dt As DataTable = Nothing
            Try

                If DAL.SelectQuery(String.Format("SELECT * FROM tbl_MembershipCategoryInfo WHERE RefNum = '{0}'", refNumber)) Then
                    dt = DAL.TableResult
                    If dt.Rows.Count > 0 Then
                        Dim value = dt.Rows(0)
                        pRefNum = refNumber
                        pPagIBIGID = value("PagIBIGID")
                        pEmployeeID = IIf(value("EmployeeID") Is DBNull.Value, String.Empty, value("EmployeeID"))
                        pEmployerName = value("EmployerName")
                        pEmployer_HBUR = IIf(value("Employer_HBUR") Is DBNull.Value, String.Empty, value("Employer_HBUR"))
                        pEmployer_Building = IIf(value("Employer_Building") Is DBNull.Value, String.Empty, value("Employer_Building"))
                        pEmployer_LotNo = IIf(value("Employer_LotNo") Is DBNull.Value, String.Empty, value("Employer_LotNo"))
                        pEmployer_BlockNo = IIf(value("Employer_BlockNo") Is DBNull.Value, String.Empty, value("Employer_BlockNo"))
                        pEmployer_PhaseNo = IIf(value("Employer_PhaseNo") Is DBNull.Value, String.Empty, value("Employer_PhaseNo"))
                        pEmployer_HouseNo = IIf(value("Employer_HouseNo") Is DBNull.Value, String.Empty, value("Employer_HouseNo"))
                        pEmployer_StreetName = IIf(value("Employer_StreetName") Is DBNull.Value, String.Empty, value("Employer_StreetName"))
                        pEmployer_Subdivision = IIf(value("Employer_Subdivision") Is DBNull.Value, String.Empty, value("Employer_Subdivision"))

                        pEmployer_Barangay = value("Employer_Barangay")
                        pEmployer_psgc_Barangay_code = value("Employer_psgc_Barangay_code")
                        pEmployer_CityMunicipality = value("Employer_CityMunicipality")
                        pEmployer_psgc_city_mun_code = value("Employer_psgc_city_mun_code")
                        pEmployer_Province = value("Employer_Province")
                        pEmployer_psgc_Province_code = value("Employer_psgc_Province_code")
                        pEmployer_Region = value("Employer_Region")
                        pEmployer_psgc_region_code = value("Employer_psgc_region_code")
                        pEmployer_ZipCode = value("Employer_ZipCode")
                        pDateEmployed = value("DateEmployed")
                        pEmployment_Status_code = value("Employment_Status_code")
                        pOccupation = value("Occupation")
                        pOccupationCode = value("OccupationCode")

                        pAFPSerialBadgeNo = IIf(value("AFPSerialBadgeNo") Is DBNull.Value, String.Empty, value("AFPSerialBadgeNo"))
                        pDepEdDivCodeStnCode = IIf(value("DepEdDivCodeStnCode") Is DBNull.Value, String.Empty, value("DepEdDivCodeStnCode"))
                        pTypeOfWork = IIf(value("TypeOfWork") Is DBNull.Value, String.Empty, value("TypeOfWork"))

                        pIncome_Code = value("Income_Code")

                        pCountry_Assignment = IIf(value("Country_Assignment") Is DBNull.Value, String.Empty, value("Country_Assignment"))
                        pNatureOfWork = value("NatureOfWork")
                        pOFWCountryCode = value("OFWCountryCode")
                        pEmpStatusCode = IIf(value("EmpStatusCode") Is DBNull.Value, String.Empty, value("EmpStatusCode"))
                        Return True
                    Else
                        pErrorMessage = "Reference Not Found."
                    End If

                Else
                    pErrorMessage = "Load MembershipCategoryInfo(): " & DAL.ErrorMessage
                End If
            Catch ex As Exception
                pErrorMessage = "Load MembershipCategoryInfo(): " & DAL.ErrorMessage
            End Try


            Return False
        End Function


